<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.stylefeng.guns.modular.system.dao.WallpaperMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.stylefeng.guns.modular.system.model.Wallpaper">
        <id column="id" property="id" />
        <result column="type_no" property="typeNo" />
        <result column="title" property="title" />
        <result column="price" property="price" />
        <result column="createtime" property="createtime" />
        <result column="version" property="version" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, type_no AS typeNo, title, price, createtime, version
    </sql>

</mapper>
